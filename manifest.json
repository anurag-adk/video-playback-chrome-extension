{
  "manifest_version": 3,
  "name": "Video Playback Controller",
  "description": "Custom playback speed for videos on any website",
  "version": "1.1.1",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
